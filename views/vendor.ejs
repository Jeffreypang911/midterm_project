<body>
  <nav class="navbar sticky-top">
    <img class="navbar-brand" src="https://i.imgur.com/EG01gRE.png">
    <form class="form-inline">
      <span class="welcome">Welcome, Pizza Owner</span>
      <input class="btn btn-outline-secondary" type="button" value="Logout" onclick="window.location.href='/'">
    </form>
  </nav>

  <div class="container">
    <div class="row">
      <div class="col-6" id="pendingcolumn">
        <h1>Pending Orders</h1>
        <div class="row" id="pendingorder">
          <div class="col-12" id='ordercontainer'>
          </div>
        </div>
      </div>
      <div class="col-6" id="completedcolumn">
        <h1>Completed Orders</h1>
        <div class="row" id="completedorder">
          <div class="col-12">
            <div id='completecontainer' class=card>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


  <footer>
      <div class="containerfooter">
          <div class="row">
              <div class="col-8">
                <img class="footerlogo" src="https://i.imgur.com/cVdvqyz.png">
              </div>
              <div class="col-2">
                <p>
                    401 W Georgia St #600
                    <br>
                    Vancouver
                    <br>
                    British Columbia
                    <br>
                    V6B 5A1
                </p>
              </div>
              <div class="col-2">
                <img class="footermap" src="https://i.imgur.com/7rQOC3R.jpg">
              </div>
          </div>
      </div>
    </footer>


